
	
	&.connectIcon_GooglePlus{
	
		span{
			&.button-group{
				a{
					&:hover{
						background: $googlePlus-color-hover;
					}
					
					&.icon{
						border-left: 1px solid $googlePlus-color-hover;
					}
				}
			}
		}
	}
	
